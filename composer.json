{
  "name": "pugkong/clibitica",
  "description": "Habitica CLI",
  "keywords": [
    "habitica",
    "cli"
  ],
  "type": "project",
  "license": "Unlicense",
  "bin": [
    "bin/clibitica"
  ],
  "require": {
    "php": ">=8.3",
    "symfony/console": "^7.2",
    "symfony/http-client": "^7.2",
    "symfony/finder": "^7.2",
    "symfony/serializer": "^7.2",
    "symfony/serializer-pack": "^1.3",
    "webmozart/assert": "^1.11"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.65",
    "phpunit/phpunit": "^11.4",
    "phpstan/phpstan": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Pug Kong",
      "email": "pugkong@tuta.io"
    }
  ]
}
