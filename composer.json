{
  "name": "pugkong/clibitica",
  "description": "Habitica CLI",
  "keywords": [
    "habitica",
    "cli"
  ],
  "type": "project",
  "license": "Unlicense",
  "bin": [
    "bin/clibitica"
  ],
  "require": {
    "php": ">=8.3",
    "symfony/console": "^7.2",
    "symfony/finder": "^7.2",
    "webmozart/assert": "^1.11",
    "symfony/rate-limiter": "^7.2",
    "symfony/lock": "^7.2",
    "symfony/cache": "^7.2",
    "symfony/type-info": "^7.2",
    "phpstan/phpdoc-parser": "^2.0",
    "pugkong/symfony-requests": "^0.0.2",
    "guzzlehttp/uri-template": "^1.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.65",
    "phpunit/phpunit": "^11.4",
    "phpstan/phpstan": "^2.0",
    "symfony/filesystem": "^7.2"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Pug Kong",
      "email": "pugkong@tuta.io"
    }
  ]
}
